  "icon": "",
  "thumbnail": "",
  "screenshots": [],
  "category": "BigBearCasaOS",
  "architectures": [
    "amd64",
    "arm64"
  ],
  "main_service": "big-bear-eufy-security-ws",
  "port_map": "3000",
  "tips": {
    "before_install": {
      "en_us": "Read this before installing: https://community.bigbeartechworld.com/t/added-eufy-security-ws-to-bigbearcasaos/2228\n"
    }
  },
  "services_metadata": {
    "big-bear-eufy-security-ws": {
      "envs": [
        {
          "name": "USERNAME",
          "description": {
            "en_us": "Eufy Account Username (required)"
          }
        },
        {
          "name": "PASSWORD",
          "description": {
            "en_us": "Eufy Account Password (required)"
          }
        },
        {
