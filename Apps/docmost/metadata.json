{
  "id": "docmost",
  "name": "Docmost",
  "description": "Docmost is an open-source alternative to the likes of Notion and Confluence. Whether you're managing a wiki, a knowledge base, or extensive project documentation, Docmost provides the tools you need to create, collaborate, and share knowledge effortlessly.",
  "tagline": "Docmost",
  "version": "0.22.2",
  "image": "docmost/docmost",
  "developer": "docmost",
  "author": "BigBearTechWorld",
  "icon": "https://cdn.jsdelivr.net/gh/bigbeartechworld/big-bear-casaos/Apps/docmost/logo.png",
  "thumbnail": "",
  "screenshots": [],
  "category": "BigBearCasaOS",
  "architectures": [
    "amd64",
    "arm64"
  ],
  "main_service": "big-bear-docmost",
  "port_map": "3000",
  "tips": {
    "before_install": {
      "en_us": "Read this: https://community.bigbeartechworld.com/t/added-docmost-to-bigbearcasaos/1724#p-3266-documentation-3\n"
    }
  },
  "services_metadata": {
    "big-bear-docmost": {
      "envs": [
        {
          "container": "APP_URL",
          "description": {
            "en_us": "Container Environment Variable: APP_URL"
          }
        },
        {
          "container": "APP_SECRET",
          "description": {
            "en_us": "Container Environment Variable: APP_SECRET"
          }
        },
        {
          "container": "DATABASE_URL",
          "description": {
            "en_us": "Container Environment Variable: DATABASE_URL"
          }
        },
        {
          "container": "REDIS_URL",
          "description": {
            "en_us": "Container Environment Variable: REDIS_URL"
          }
        }
      ],
      "volumes": [
        {
          "container": "/app/data/configs",
          "description": {
            "en_us": "Container Path: /app/data/configs"
          }
        },
        {
          "container": "/app/public/icons",
          "description": {
            "en_us": "Container Path: /app/public/icons"
          }
        },
        {
          "container": "/data",
          "description": {
            "en_us": "Container Path: /data"
          }
        }
      ],
      "ports": [
        {
          "container": "3000",
          "description": {
            "en_us": "Container Port: 3000"
          }
        }
      ]
    },
    "big-bear-docmost-db": {
      "envs": [
        {
          "container": "POSTGRES_DB",
          "description": {
            "en_us": "Container Environment Variable: POSTGRES_DB"
          }
        },
        {
          "container": "POSTGRES_USER",
          "description": {
            "en_us": "Container Environment Variable: POSTGRES_USER"
          }
        },
        {
          "container": "POSTGRES_PASSWORD",
          "description": {
            "en_us": "Container Environment Variable: POSTGRES_PASSWORD"
          }
        }
      ],
      "volumes": [
        {
          "container": "/var/lib/postgresql/data",
          "description": {
            "en_us": "Container Path: /var/lib/postgresql/data"
          }
        }
      ]
    },
    "big-bear-docmost-redis": {
      "volumes": [
        {
          "container": "/data",
          "description": {
            "en_us": "Container Path: /data"
          }
        }
      ]
    }
  },
  "links": {
    "big_bear_casaos_youtube": "",
    "docs": "",
    "big_bear_cosmos_youtube": ""
  }
}